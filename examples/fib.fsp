;; Fibonacci function

(func fib (n)
	(if (< n 2) n (+ (fib (- n 1)) (fib (- n 2)))))

;; main doesn't run when this file is loaded as a file
;; it only runs when this file is executed directly
;; filename must currently be pre-pended to main until modules are implemented

(func fib.main ()
	(println (fib 41)))
